{
  "name": "userdive-examples",
  "version": "1.2.0",
  "devDependencies": {
    "@types/query-string": "6.1.0",
    "gh-pages": "1.2.0",
    "pug": "2.0.3",
    "pug-cli": "1.0.0-alpha6",
    "query-string": "6.1.0",
    "testcafe": "0.20.4"
  },
  "license": "MIT",
  "main": "n/a",
  "private": true,
  "scripts": {
    "build": "run-p build:*",
    "build:html": "pug . -o build/agent.js -P -s",
    "build:js": "webpack --env production",
    "clean": "rimraf tests build",
    "e2e": "testcafe 'chrome --no-sandobox' test/",
    "lint": "run-p lint:*",
    "lint:pug": "pug-lint ./**/*.pug",
    "lint:ts": "tslint ./**/*.ts{,x}",
    "prebuild": "npm run clean",
    "start": "concurrently -k 'yarn w:ts' 'yarn w:html'",
    "w:html": "pug . -o build/agent.js -P -s -w",
    "w:ts": "webpack-dev-server --env production"
  }
}
