  ],
  "license": "GPL-3.0",
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --parser typescript --write",
      "tslint --fix",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "lib/agent.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/userdive/agent.js.git"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:babel": "cross-env NODE_ENV=dev tsc && webpack",
    "build:bundle": "cross-env NODE_ENV=production tsc && webpack --profile --json | webpack-bundle-size-analyzer",
    "build:doc": "esdoc",
    "ci:sauce": "cross-env CI_MODE=sauce npm run test",
    "clean": "rimraf coverage lib docs",
    "coverage": "codecov -t $CODECOV_TOKEN -f ./coverage/*/lcov.info",
    "lint": "tslint \"src/*.ts\" \"test/*.ts\"",
    "postbuild": "cp robots.txt lib/build/.",
    "prebuild": "npm run clean",
    "precommit": "lint-staged",
    "prepublish": "npm run build",
    "test": "run-p  test:*",
    "test:build": " run-s clean build",
    "test:karma": "karma start --single-run",
    "test:lint": "npm run lint"
  }
}
