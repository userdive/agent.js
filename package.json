    "@uncovertruth/tslint-config": "^1.3.0",
    "assert-exception": "^1.1.0",
    "codecov": "^3.0.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "faker": "^4.1.0",
    "husky": "^0.14.3",
    "is-url": "^1.2.2",
