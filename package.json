    "ui-event-observer": "^1.2.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "@types/faker": "^4.1.2",
    "@types/is-url": "^1.2.28",
    "@types/js-cookie": "^2.1.0",
