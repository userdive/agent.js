    "@types/webpack": "4.0.0",
    "@uncovertruth/eslint-config": "4.0.0",
    "@uncovertruth/eslint-config-react": "4.0.0",
    "@uncovertruth/stylelint-config": "4.1.0",
    "@uncovertruth/textlint-config-en": "4.0.0",
    "@uncovertruth/tslint-config": "4.0.0",
    "assert-exception": "1.1.0",
    "can-npm-publish": "1.0.2",
    "codecov": "3.0.0",
    "concurrently": "3.5.1",
    "cross-env": "5.1.4",
    "eslint": "4.19.0",
    "faker": "4.1.0",
    "fixpack": "2.3.1",
    "husky": "0.14.3",
    "is-url": "1.2.3",
    "istanbul-instrumenter-loader": "3.0.0",
    "karma": "2.0.0",
    "karma-coverage-istanbul-reporter": "1.4.2",
    "karma-fixture": "0.2.6",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-sauce-launcher": "1.2.0",
    "karma-webpack": "2.0.13",
    "lerna": "2.9.0",
    "lerna-changelog": "0.7.0",
    "lint-staged": "7.0.0",
    "mocha": "5.0.4",
    "npm-run-all": "4.1.2",
    "power-assert": "1.4.4",
    "prettier-eslint-cli": "4.7.1",
    "pug-lint": "2.5.0",
    "pug-lint-config-uncovertruth": "4.0.0",
    "rimraf": "2.6.2",
    "sinon": "4.4.6",
    "size-limit": "0.16.2",
    "stylelint": "9.1.3",
    "textlint": "10.1.4",
    "ts-loader": "3.5.0",
    "ts-node": "5.0.1",
    "tslint": "5.9.1",
    "tslint-react": "3.5.1",
    "typescript": "2.7.2",
    "typescript-eslint-parser": "14.0.0",
    "uglify-js": "3.3.16",
    "uglifyjs-webpack-plugin": "1.2.3",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.2",
    "webpack-espower-loader": "2.0.0"
  },
  "engine": {
    "node": "> 5"
  },
  "homepage": "https://github.com/userdive/agent.js#readme",
  "keywords": [
    "userdive"
  ],
  "license": "GPL-3.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/userdive/agent.js.git"
  },
  "scripts": {
    "build": "lerna run build --parallel",
    "bump": "lerna publish --skip-npm --skip-git && lerna-changelog",
    "ci:appveyor": "cross-env CI_MODE=IE lerna run test",
    "ci:sauce": "cross-env CI_MODE=sauce lerna run test",
    "clean": "lerna clean --yes && lerna run clean && rimraf cdn",
    "coverage": "run-p coverage:*",
    "coverage:agent": "codecov -f ./packages/agent/coverage/lcov.info -F agent",
    "coverage:linker": "codecov -f ./packages/linker/coverage/lcov.info -F linker",
    "coverage:userdive": "codecov -f ./packages/userdive/coverage/lcov.info -F userdive",
    "e2e": "lerna run e2e",
    "examples": "lerna run dev --scope userdive-examples",
    "lint": "run-p lint:*",
    "lint:css": "stylelint 'website/static/**/*.{css,scss}'",
    "lint:examples": "tslint -c ./examples/tslint.json 'examples/**/*.ts{,x}'",
    "lint:js": "eslint -c website/.eslintrc.json website",
    "lint:md": "textlint 'packages/*/*.md' README.md docs",
    "lint:pug": "pug-lint examples/**/*.pug",
    "lint:ts": "tslint 'packages/*/{src,test}/**/*.ts'",
    "postbuild": "webpack --env=production && size-limit && find cdn -type f | grep -v js | xargs rm && cp robots.txt ./cdn",
    "precommit": "lint-staged",
    "prepare": "lerna run build --scope userdive && lerna run build --scope @userdive/*",
    "release": "lerna exec --bail=false -- 'can-npm-publish && npm publish'",
    "start": "concurrently --kill-others 'npm run examples' 'npm run website'",
    "test": "lerna run test",
    "website": "cd website && npm start"
  },
  "size-limit": [
    {
      "path": "cdn/agent.js",
      "limit": "8.2 KB"
    },
    {
      "path": "cdn/agent.d.js",
      "limit": "20 KB"
    },
    {
      "path": "cdn/linker.js",
      "limit": "1.1 KB"
    }
  ],
  "workspaces": [
    "packages/*",
    "website",
    "examples/*",
    "examples"
  ]
}
